    "jest": "^25.3.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.0.4",
    "sqlite": "^4.0.6",
    "sqlite3": "^4.1.1",
    "ts-jest": "^25.3.1",
    "typeorm": "^0.2.24",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "faker": "4.1.0",
    "glob": "7.1.6",
    "ora": "4.0.3",
    "reflect-metadata": "0.1.13",
    "yargs": "15.3.1"
  },
